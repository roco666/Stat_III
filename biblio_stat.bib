@BOOK{Wild2000,
  title = {{Chance encounters. A first course in data analysis and inference.}},
  publisher = {John Wiley \& Sons},
  year = {2000},
  author = {Wild, Christopher J. and Seber, George A.F.},
  address = {New York, NY},
  optabstract = {{This is a precalculus text which assumes no prior knowledge of probability
	and statistics. It is written in an informal, non-mathematical style.
	Each chapter is introduced through exploring data from real life
	examples from many areas and is completed with summaries of the main
	points, quizzes and revision exercises, solutions to some of which
	are given later. Extended case studies are used to illustrate more
	sophisticated techniques. Early chapters consider exploring single
	variables and relationships between pairs of variables. These are
	followed by probability, including conditional probability, discrete
	random variables where the binomial distribution is the only standard
	distribution discussed and continuous random variables where only
	the normal distribution is described in detail. The chapters on inference
	commence with sampling distributions including the $t$-distribution
	and the central limit theorem and normal based confidence intervals
	and hypothesis testing; nonparametric tests are also discussed. Contingency
	tables and regression and correlation complete the book. Further
	chapters on control charts and time series are available on a website
	(www.wiley.com/college/wild) in addition to further data, exercises
	and slides.}},
  optnote = {Ne va pas très loin, recommandé par Statoo}
}

